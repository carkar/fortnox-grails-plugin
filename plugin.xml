<plugin name='fortnox' version='0.1' grailsVersion='2.1.5 &gt; *'>
  <author>Carljohan Carlsson</author>
  <authorEmail>carljohan.carlsson@gmail.com</authorEmail>
  <title>Fortnox</title>
  <description>Grails plugin for integrating with Fortnox (www.fortnox.se) API</description>
  <documentation>https://github.com/carkar/fortnox-grails-plugin</documentation>
  <type>FortnoxGrailsPlugin</type>
  <resources>
    <resource>com.matchi.fortnox.services.FortnoxContactService</resource>
    <resource>com.matchi.fortnox.services.FortnoxInvoiceService</resource>
    <resource>com.matchi.fortnox.services.FortnoxItemService</resource>
    <resource>com.matchi.fortnox.services.FortnoxService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='http://repository.codehaus.org' url='http://repository.codehaus.org/' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://maven.springframework.org/release' url='http://maven.springframework.org/release/' />
    <repository name='http://maven.springframework.org/snapshot' url='http://maven.springframework.org/snapshot/' />
    <repository name='http://maven.springframework.org/milestone' url='http://maven.springframework.org/milestone/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
    <repository name='http://download.java.net/maven/2/' url='http://download.java.net/maven/2/' />
    <repository name='http://repo.grails.org/grails/libs-releases' url='http://repo.grails.org/grails/libs-releases/' />
    <repository name='http://m2repo.spockframework.org/ext/' url='http://m2repo.spockframework.org/ext/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.jadira.usertype' name='usertype.jodatime' version='1.9' />
    </compile>
    <runtime>
      <dependency group='org.codehaus.groovy.modules.http-builder' name='http-builder' version='0.5.1' />
      <dependency group='bankgiro-api' name='bankgiro-api' version='1.3.2' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='joda-time' version='1.4' />
      <plugin group='org.grails.plugins' name='jxl' version='0.54' />
      <plugin group='org.grails.plugins' name='spring-social-core' version='0.1.31' />
      <plugin group='org.grails.plugins' name='rendering' version='0.4.4' />
      <plugin group='org.grails.plugins' name='webflow' version='2.0.0' />
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='mail' version='1.0.1' />
      <plugin group='org.grails.plugins' name='spring-social-facebook' version='0.1.32' />
      <plugin group='org.grails.plugins' name='lesscss-resources' version='1.3.0.3' />
      <plugin group='org.grails.plugins' name='quartz' version='1.0-RC8' />
      <plugin group='org.grails.plugins' name='csv' version='0.3.1' />
      <plugin group='org.grails.plugins' name='cxf-client' version='1.2.9' />
      <plugin group='org.grails.plugins' name='facebook-graph' version='0.14' />
      <plugin group='org.grails.plugins' name='marshallers' version='0.3' />
      <plugin group='org.grails.plugins' name='jquery-ui' version='1.8.15' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.1.5' />
      <plugin group='org.grails.plugins' name='asynchronous-mail' version='1.0-RC5' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='zipped-resources' version='1.0' />
      <plugin group='org.grails.plugins' name='constraints' version='0.6.0' />
      <plugin group='org.grails.plugins' name='jquery' version='1.7.2' />
      <plugin group='org.grails.plugins' name='jquery-validation' version='1.7.3' />
      <plugin group='org.grails.plugins' name='resources' version='1.2.RC2' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.1.5' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>